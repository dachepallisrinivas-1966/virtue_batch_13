public class SwitchDemo {

	public static void main(String[] args) {
		double a = 10.5;
		boolean b = true;
		
		switch (b) {
//		case 10.5 : System.out.println("it is 10.5");
//		case 11.5 : System.out.println("it is not 10.5");
		}

		if (a == 10.5)
			System.out.println("it is 10.5");
		else
			System.out.println("it is not 10.5");
	}

}
